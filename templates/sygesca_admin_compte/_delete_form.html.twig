<form method="post" action="{{ path('sygesca_admin_compte_delete', {'id': compte.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer ce compte?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ compte.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
